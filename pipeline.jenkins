pipeline {
   agent any

   stages {
     stage('build') {
        steps {
            bat "\"${tool 'MSBuild'}\" WindowsFormsApp2.sln"
        }
     }
     stage('CopyToExchange')
     {
         steps
         {
            bat "xcopy /s '${WORKSPACE}/${BRANCH_NAME}/Bin/' 'F:/FinalBuilds'"
         }
     }
   }
}
