pipeline {
   agent any

   stages {
     stage('build') {
        steps {
            bat "\"${tool 'MSBuild'}\" WindowsFormsApp2.sln"
        }
     }
     stage('CopyToExchange')
     {
         steps
         {
            sh 'xcopy "${WORKSPACE}\${BRANCH_NAME}\Bin\" "F:\FinalBuilds\" /O /X /E /H /K'
         }
     }
   }
}
