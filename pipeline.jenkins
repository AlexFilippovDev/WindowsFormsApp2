pipeline {
   agent any

   stages {
     stage('build') {
        steps {
            bat "\"${tool 'MSBuild'}\" WindowsFormsApp2.sln"
        }
     }
     stage('CopyToExchange')
     {
         steps
         {
            echo "${WORKSPACE}"
            bat "xcopy ""${WORKSPACE}\\${BRANCH_NAME}\\Bin""  ""F:\\FinalBuilds\\"" /E"
         }
     }
   }
}
